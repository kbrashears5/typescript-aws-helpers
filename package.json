    "main": "lib/index.js",
    "license": "MIT",
    "dependencies": {
        "typescript-aws-apigateway-helper": "~1.0.89",
        "typescript-aws-cloudwatch-helper": "~1.0.78",
        "typescript-aws-dynamo-helper": "~1.0.82",
        "typescript-aws-kms-helper": "~1.0.84",
        "typescript-aws-lambda-helper": "~1.0.76",
        "typescript-aws-s3-helper": "~1.0.86",
        "typescript-aws-ses-helper": "~1.0.80",
        "typescript-aws-sns-helper": "~1.0.77",
        "typescript-aws-sqs-helper": "~1.0.82",
        "typescript-aws-ssm-helper": "~1.0.80",
        "typescript-aws-sts-helper": "~1.0.82",
        "typescript-helper-functions": "~1.0.54",
        "typescript-ilogger": "~1.0.23",
        "uuid": "~8.3.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "~8.10.62",
        "@types/node": "~14.6.4",
        "aws-sdk": "~2.755.0",
