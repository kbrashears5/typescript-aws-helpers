        "rimraf": "~3.0.2",
        "tslint": "~6.1.3",
        "typescript": "~4.0.2"
    },
    "files": [
        "lib/**"
    ],
    "scripts": {
        "prebuild": "rimraf ./lib",
        "build": "./node_modules/.bin/tsc",
